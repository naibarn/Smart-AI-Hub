{
  "name": "smart-ai-hub",
  "version": "1.0.0",
  "description": "Monorepo for Smart AI Hub services",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "build": "npm-run-all --serial build:*",
    "start": "npm-run-all --parallel start:*",
    "install:all": "npm install && npm-run-all install:*"
  },
  "devDependencies": {
    "typescript": "^5.5.4",
    "@types/node": "^22.4.1",
    "npm-run-all": "^4.1.5"
  }
  
}